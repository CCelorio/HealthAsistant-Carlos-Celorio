<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Eleccion.Notificaciones.NotificacionesActivity">

    <TextView
        android:id="@+id/nombreUsuarioTextView4"
        android:layout_width="262dp"
        android:layout_height="47dp"
        android:layout_marginTop="15dp"
        android:fontFamily="cursive"
        android:text="TextView"
        android:textAlignment="viewStart"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="66dp"
        android:layout_height="54dp"
        android:layout_marginTop="1dp"
        android:src="@drawable/_326055"
        app:layout_constraintEnd_toStartOf="@+id/nombreUsuarioTextView4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnCerrarSesion"
        android:layout_width="129dp"
        android:layout_height="48dp"
        android:layout_margin="8dp"
        android:backgroundTint="#E82020"
        android:onClick="cerrarSesion"
        android:padding="16dp"
        android:radius="8dp"
        android:text="Cerrar SesiÃ³n"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>